{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/react"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/favicon.ico",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/((?!api/).*)",
      "dest": "/index.html",
      "methods": ["GET", "HEAD"]
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://af-mart828.vercel.app/api",
    "REACT_APP_BACKEND_URL": "https://af-mart828.vercel.app",
    "REACT_APP_FRONTEND_URL": "https://afmart.vercel.app"
  }
}

